{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "OpenAI": "https://<OPENAI_CUSTOM_DOMAIN>.openai.azure.com/"
  },
  "AppConfiguration": {
    "AISearchClient": {
      "endpoint": "https://<SEARCH_SERVICE>.search.windows.net",
      "indexName": "contoso-product-index",
      "semanticConfigName": "product-semantic-config",
      "vectorFieldNames": [
        "description_vectorized",
        "brand_vectorized"
      ],
      "nearestNeighbours": 3,
      "fields": [
        "name",
        "description"
      ]
    },
    "OpenAIClient": {
      "embeddingClientName": "text-embedding-ada-002",
      "model": "gpt-4o",
      "systemPromptFileName": "system_prompt.txt"
    }
  }
}